<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="Cantitate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/previewFinalCantitate"
        android:layout_marginStart="16dp"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_alignBottom="@+id/textView19"
        android:layout_toEndOf="@+id/textView19"
        android:textStyle="normal|bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/previewFinalDenumire"
        android:text="denumire"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:maxLines="2"
        android:lines="2"
        android:textColor="@color/colorPrimary" />

    <TextView
        android:text="CANTITATE"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_below="@+id/previewFinalDenumire"
        android:layout_alignParentStart="true" />
</RelativeLayout>
